{
  "name": "service-template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Service template",
  "main": "index.js",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node ./dist/src/index.js",
    "dev": "npx nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/index.ts",
    "client": "node --experimental-specifier-resolution=node --loader ts-node/esm src/test.ts",
    "build": "tsc",
    "generate-types": "proto-loader-gen-types --grpcLib=@grpc/grpc-js --keepCase --longs=String --enums=String --defaults --oneofs --outDir=types/proto/ ../proto/users/*.proto ../proto/auth/*.proto ../proto/products/*.proto"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.15",
    "@types/node": "18.11.18",
    "@types/uuid": "9.0.1",
    "nodemon": "2.0.20",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "uuid": "9.0.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.8.0",
    "@grpc/proto-loader": "0.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.18.2"
  }
}
