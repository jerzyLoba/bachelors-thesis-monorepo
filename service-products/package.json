{
  "name": "service-template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Service template",
  "main": "index.js",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node ./dist/index.js",
    "dev": "npx nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/index.ts",
    "build": "tsc -b",
    "generate-types": "proto-loader-gen-types --grpcLib=@grpc/grpc-js --keepCase --longs=String --enums=String --defaults --oneofs --outDir=src/proto/ ../proto/products/*.proto"
  },
  "keywords": [],
  "author": "jloba",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/node": "18.11.18",
    "@types/pg": "8.6.6",
    "nodemon": "2.0.20",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.8.22",
    "@grpc/proto-loader": "0.7.4",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "pg": "8.8.0",
    "prom-client": "14.2.0"
  }
}
