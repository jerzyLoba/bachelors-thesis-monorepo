{
  "name": "service-auth",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Service auth",
  "main": "index.js",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node ./dist/src/index.js",
    "dev": "npx nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/index.ts",
    "build": "tsc",
    "generate-types": "proto-loader-gen-types -v --grpcLib=@grpc/grpc-js --keepCase --longs=String --enums=String --defaults --oneofs --outDir=types/proto/ ../proto/users/*.proto ../proto/auth/*.proto"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "18.11.18",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "dotenv": "16.0.3",
    "@grpc/grpc-js": "1.8.0",
    "@grpc/proto-loader": "0.7.4",
    "bcrypt": "5.1.0"
  }
}
